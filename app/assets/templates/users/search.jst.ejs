<h4>Add Friends</h4>
<input type="text" id="search-field"> <button>Add Friends</button>
<% var currentFriends = [CURRENT_USER.id] %>
<% users.get(CURRENT_USER.id).friends().each(function (friend) { %>
  <% currentFriends.push(friend.id) %>
<% }); %>
<div id="search-results">
  <ul>
    <% users.each(function (user) { %>
      <% if (currentFriends.indexOf(user.get('id')) === -1 ) { %>
        <li class="search-items" data-id="<%= user.escape('id') %>"><%= user.escape('email') %></li>
      <% } %>
    <% }); %>
  </ul>
</div>
